                        <apex:form >
                            <header class="slds-global-header_container slds-text-align_center">
                                <div class="slds-global-header">
                                    <apex:commandButton action="{!cancel}" value="Cancel"
                                                        rendered="{!saveCancelButton}"/>
                                    <apex:commandButton action="{!Save}" value="Save" rendered="{!saveCancelButton}"
                                                        styleclass="slds-button slds-button_brand"/>
                                </div>
                            </header>

                            <apex:inputField value="{!Contract.Commentary_FAI__c}" style="width: 3500px;"/>
                            <div class="slds-docked-form-footer slds-text-align_center"
                                 style="display:{!commentaryVarInput}; background-color:white; z-index: 999;">
                                <apex:commandButton action="{!cancel}" value="Cancel" rendered="{!saveCancelButton}"/>
                                <apex:commandButton action="{!Save}" value="Save" rendered="{!saveCancelButton}"
