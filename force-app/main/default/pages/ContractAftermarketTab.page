                    <div class="slds-form-element__control">
                        <div class="slds-form-element__static">
                            <span class="slds-form-element__label"> <apex:outputLabel value="{!$ObjectType.Contract.fields.Commentary_FAI__c.label}"/></span>
                            <apex:outputField value="{!Contract.Commentary_FAI__c}"/>
                        </div>
                        <button class="slds-button slds-button_icon" title="Edit" onclick="callClass()">
                            <svg class="slds-button__icon slds-button__icon_hint" aria-hidden="true">
                                <use xlink:href="{!URLFOR($Asset.SLDS,'/assets/icons/utility-sprite/svg/symbols.svg#edit')}"
                                     onclick="callClass()"></use>
                            </svg>
                            <span class="slds-assistive-text">Edit</span>
                        </button>
                    </div>
                </div>
            </div>

            <div class="slds-form__item" role="listitem" style="display:{!commentaryVarInput}">
